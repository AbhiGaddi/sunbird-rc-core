<http://example.com/voc/teacher/1.0.0/>
  a owl:Ontology ;
  dc11:description "W3C RDF Schema based sample vocabulary."^^xsd:string ;
  dc11:identifier "OpenSaber Sample Vocabulary v1.0.0"^^xsd:string ;
  dc11:title "OpenSaber Sample Vocabulary"^^xsd:string ;
  dc:issued "2018-1-16"^^xsd:date .

<http://example.com/voc/teacher/1.0.0/Place>
  a owl:Class, rdfs:Class ;
  rdfs:comment "Entities that have a somewhat fixed, physical location."@en ;
  rdfs:isDefinedBy <http://example.com/voc/teacher/1.0.0/> ;
  rdfs:label "Place"@en ;
  rdfs:subClassOf owl:Thing ;
  ns0:term_status "testing"^^xsd:string ;
  skos:broadMatch schema:Place .

<http://example.com/voc/teacher/1.0.0/Organization>
  a owl:Class, rdfs:Class ;
  rdfs:comment "An Organization is any legal or physical entity."@en ;
  rdfs:isDefinedBy <http://example.com/voc/teacher/1.0.0/> ;
  rdfs:label "Organization"@en ;
  rdfs:subClassOf owl:Thing ;
  ns0:term_status "testing"^^xsd:string ;
  skos:exactMatch schema:Organization .

<http://example.com/voc/teacher/1.0.0/School>
  a owl:Class, rdfs:Class ;
  rdfs:comment "A school is where one goes to learn. A School is uniquely identified by a U-DISE Code."@en ;
  rdfs:isDefinedBy <http://example.com/voc/teacher/1.0.0/> ;
  rdfs:label "School"@en ;
  rdfs:subClassOf <http://example.com/voc/teacher/1.0.0/Organization> ;
  ns0:term_status "testing"^^xsd:string ;
  skos:exactMatch schema:School .

<http://example.com/voc/teacher/1.0.0/udiseNumber>
  a rdf:Property, owl:DatatypeProperty ;
  rdfs:comment "A U-DISE Code is a Number given to each school covered under U-DISE. It is a unique code given in the first year of coverage under U-DISE and used every year to ensure compatibilityand comparison across years."@en ;
  rdfs:domain <http://example.com/voc/teacher/1.0.0/School> ;
  rdfs:isDefinedBy <http://example.com/voc/teacher/1.0.0/> ;
  rdfs:label "U-DISE Code (UDISE)"@en ;
  rdfs:range xsd:integer ;
  ns0:term_status "testing"^^xsd:string ;
  skos:exactMatch schema:globalLocationNumber .

<http://example.com/voc/teacher/1.0.0/academicCalendarYearStart>
  a rdf:Property, owl:DatatypeProperty ;
  rdfs:comment "The calendar year in which the Academic year starts."@en ;
  rdfs:domain <http://example.com/voc/teacher/1.0.0/School> ;
  rdfs:isDefinedBy <http://example.com/voc/teacher/1.0.0/> ;
  rdfs:label "Academic Calendar Year Start"@en ;
  rdfs:range xsd:gYear ;
  ns0:term_status "testing"^^xsd:string .

<http://example.com/voc/teacher/1.0.0/academicCalendarYearEnd>
  a rdf:Property, owl:DatatypeProperty ;
  rdfs:comment "The calendar year in which the Academic year ends."@en ;
