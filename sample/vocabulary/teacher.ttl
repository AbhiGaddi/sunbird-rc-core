  rdfs:range <http://example.com/voc/teacher/1.0.0/AreaTypeCode> ;
  ns0:term_status "testing"^^xsd:string .

<http://example.com/voc/teacher/1.0.0/address>
  a owl:ObjectProperty, rdf:Property ;
  rdfs:comment "An Indian postal address."@en ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     <http://example.com/voc/teacher/1.0.0/Organization>
     <http://example.com/voc/teacher/1.0.0/Place>
   )
  ] ;
  rdfs:isDefinedBy <http://example.com/voc/teacher/1.0.0/> ;
  rdfs:label "Has Postal Address"@en ;
  rdfs:range <http://example.com/voc/teacher/1.0.0/IndianPostalAddress> ;
  ns0:term_status "testing"^^xsd:string ;
  skos:exactMatch schema:address .

